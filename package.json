{
    "name": "seekode-theme",
    "displayName": "seekode theme",
    "description": "a theme package made by seekode",
    "icon": "images/dark.png",
    "version": "0.5.0",
    "author": "seekode",
    "publisher": "seekode",
    "repository": {
        "type": "git",
        "url": "https://github.com/seekode/seekode-vsc-theme.git/"
    },
    "engines": {
